#!/bin/bash

old_names=$1
new_names=$2
profile=$3
bucket=$4

exec 3<$old_names
exec 4<$new_names

while
  IFS= read -r old <&3
  IFS= read -r new <&4
do
  echo "aws s3 mv s3://$bucket/$old s3://$bucket/$new"
  aws --profile $profile s3 mv "s3://$bucket/$old" "s3://$bucket/$new"
  sleep 1
done
