#!/bin/bash

POSITIONAL=()
while [[ $# -gt 0 ]]
do
key="$1"

case $key in
    -x|--execute)
    REAL_RUN=1
    shift # past argument
    ;;
    *)    # unknown option
    POSITIONAL+=("$1") # save it in an array for later
    shift # past argument
    ;;
esac
done
set -- "${POSITIONAL[@]}"

echo "REAL $REAL_RUN";

old_names=$1
new_names=$2

echo "$old_names $new_names";

exec 3<$old_names
exec 4<$new_names

while
  IFS= read -r old <&3
  IFS= read -r new <&4
do
  if [[ $REAL_RUN ]];
  then
      mv "$old" "$new";
  else
      echo -e "Dryrun mv [$old] [$new]";
  fi
done
